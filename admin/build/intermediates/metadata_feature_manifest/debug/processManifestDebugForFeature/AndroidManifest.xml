<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    featureSplit="admin"
    package="com.example.kababistanresturant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:targetSdkVersion="24" />

    <dist:module
        dist:instant="false"
        dist:title="Kababistan Admin" >
        <dist:delivery>
            <dist:on-demand />
        </dist:delivery>

        <dist:fusing dist:include="true" />
    </dist:module>

    <application
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:testOnly="true" >
        <activity
            android:name="com.example.kababistanresturant.AdminMainActivity"
            android:exported="true"
            android:label="Kababistan Admin"
            android:splitName="admin"
            android:theme="@style/Theme.KababistanApp" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>